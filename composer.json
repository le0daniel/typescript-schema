{
  "name": "le0daniel/typescript-schema",
  "license": "MIT",
  "type": "package",
  "require": {
    "php": "^8.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.2",
    "phpstan/phpstan": "^1.12"
  },
  "autoload": {
    "psr-4": {
      "TypescriptSchema\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TypescriptSchema\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "check": "phpstan analyse src --level 8"
  }
}
